{
    "name": "solidbase/geometria",
    "description": "MÃ³dulo de Geometria do SolidBase",
    "type": "packpage",
    "autoload": {
        "psr-4": {
            "Solidbase\\Geometry\\": "src/"
        },
        "files": [
            "src/Utils/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Guilherme Luiz",
            "email": "guilherme@solidbase.com.br"
        }
    ],
    "require": {
        "php": "^8.1",
        "doctrine/collections": "^2.1",
        "solidbase/mathematics": "^0.0.2"
