    "config": {
        "process-timeout": 0,
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "start": "php -S api.solidbase:8080 -t public",
        "test": "./vendor/bin/pest"
    },
    "require-dev": {
        "pestphp/pest": "^2.0",
        "mockery/mockery": "^1.6",
        "symfony/var-dumper": "^6.3.0"
    }
}
